// control structures
for x in range(0,1) {
     print(x);
}

let i = 0;
while i <= 10 {
     print(i);
     i = i+1;
     if i == 10 {
          break;
     } elif i == 11 {
          // never happens
     } else {
          print('not 10 yet!');
     }
}

// functions
fn greet(name) {
     return 'Hello, ' + name + '!';
}

let greet2 = fn(name) {
     return 'Hello again, ' + name + '!';
};

print(greet('Jake'));
print(greet2('Jake'));

